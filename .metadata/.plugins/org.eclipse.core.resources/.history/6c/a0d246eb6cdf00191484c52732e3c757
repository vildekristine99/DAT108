package no.hvl.dat108;

import java.util.ArrayList;
import java.util.List;

public class Liste {
    
    private List<Vare> varer = new ArrayList<>();
    
    public Liste() {
    	
    }
    
    public void addVare(Vare vare) {
        this.varer.add(vare);
    }
    
    public List<Vare> getVarer() {
        return varer;
    } 
    
}